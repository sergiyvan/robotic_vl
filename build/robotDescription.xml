							</effector>
						</effector>
					</effector>
				</effector>

				<!-- right arm -->
				<effector id="4" name="RightArmPitch" type="rotation" position="0 -85.5 0" rpy="90 0 -90" defaultMinMaxAngle="0 -150 150" maxForce="3.7" maxSpeed="80">
					<body mass="111" name="leftArmPitchMotor" position="8 0 0"/>

					<visual>
